{
  "name": "getir-case-study",
  "version": "1.0.0",
  "description": "Getir Case study assignment",
  "main": "index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "node ./index.js",
    "dev": "nodemon ./index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "test": "LOG_LEVEL=silent jest --runInBand --forceExit --detectOpenHandles",
    "precommit": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VipulKumarShukla/getir-case-study.git"
  },
  "author": "Vipul Kumar Shukla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VipulKumarShukla/getir-case-study.git/issues"
  },
  "homepage": "https://github.com/VipulKumarShukla/getir-case-study#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "@types/jest": "^27.0.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "jest": "^26.6.3",
    "joi": "^17.4.2",
    "mockingoose": "^2.15.2",
    "mongoose": "^5.10.0",
    "pino": "^6.13.0",
    "pino-pretty": "^5.1.3",
    "supertest": "^6.1.5",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.5"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/**/?(*.)spec.js"
    ],
    "collectCoverageFrom": [
      "./*.js",
      "./**/*.js",
      "!./index.js",
      "!./controllers/v1/test/integration/*.js",
      "!./coverage/**/*.js",
      "!./plugins/*.js",
      "!./config/*.js"
    ]
  }
}
